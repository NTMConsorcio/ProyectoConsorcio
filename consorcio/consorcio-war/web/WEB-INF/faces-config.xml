<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <lifecycle>
        <phase-listener>com.consorcio.controller.ControllerAutenticacion</phase-listener>
    </lifecycle>
    
    <navigation-rule> 
        <navigation-case>
            <from-outcome>editPais</from-outcome>
            <to-view-id>/admin/pais/editPais.jsf</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <navigation-case>
            <from-outcome>editExpensa</from-outcome>
            <to-view-id>/admin/expensa/editExpensa.jsf</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
   
    <navigation-rule> 
        <navigation-case>
            <from-outcome>listExpensa</from-outcome>
            <to-view-id>/admin/expensa/listExpensa.jsf</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <navigation-case>
            <from-outcome>listPais</from-outcome>
            <to-view-id>/admin/pais/listPais.jsf</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <navigation-case>
            <from-outcome>listInmueble</from-outcome>
            <to-view-id>/admin/inmueble/listInmueble.jsf</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule> 
        <navigation-case>
            <from-outcome>editInmueble</from-outcome>
            <to-view-id>/admin/inmueble/editInmueble.jsf</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
